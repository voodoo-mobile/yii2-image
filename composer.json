{
	"name": "voodoo-rocks/yii2-image",
	"type": "library",
	"description": "The library for managing images",
	"keywords": [
		"images"
	],
	"homepage": "https://github.com/voodoo-rocks/yii2-image",
	"license": "MIT",
	"authors": [
		{
			"name": "Voodoo Rocks Ltd",
			"email": "public@voodoo.rocks",
			"homepage": "http://voodoo.rocks"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php": ">=5.4.0",
		"yiisoft/yii2": "~2.0",
		"voodoo-rocks/yii2-upload": "~3.0",
		"yiisoft/yii2-imagine": "*"
	},
	"autoload": {
		"psr-0": {
			"Image": "src"
		},
		"psr-4": {
			"vr\\image\\": "src/"
		}
	},
	"extra": {
		"asset-installer-paths": {
			"npm-asset-library": "vendor/npm",
			"bower-asset-library": "vendor/bower"
		},
		"writable": [
			"runtime"
		]
	}
}
